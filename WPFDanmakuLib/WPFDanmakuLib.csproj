<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net6.0-windows</TargetFramework>
    <OutputType>Library</OutputType>
    <UseWPF>true</UseWPF>
    <Nullable>enable</Nullable>
    <Authors>StoneMoe</Authors>
    <PackageProjectUrl>https://github.com/luojunyuan/WPFDanmakuLib</PackageProjectUrl>
    <RepositoryUrl>https://github.com/StoneMoe/WPFDanmakuLib</RepositoryUrl>
    <Description></Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <RepositoryType>Extention</RepositoryType>
    <PackageTags>danmaku, danmu, bullet curtain</PackageTags>
    <Version>0.0.0.1</Version>
    <PackageId>luojunyuan.WpfDanmaku</PackageId>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>
</Project>